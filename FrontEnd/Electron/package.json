{
  "name": "HeepDesktop",
  "productName": "Heep Desktop",
  "version": "0.0.0",
  "private": false,
  "license": "see LICENSE file",
  "description": "A Minimal Hello World for JS Frameworks",
  "author": "kacobjeith",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "engines": {
    "node": "7.7.4"
  },
  "scripts": {
    "tests": "mocha-webpack --webpack-config webpack.config-test.js \"src/**/*.test.js\"",
    "build": "webpack",
    "dev": "webpack-dev-server --progress --inline --hot",
    "start": "babel-node src/server.js",
    "tbs": "npm run test && npm run bs",
    "bs": "webpack && npm run start",
    "ts": "npm run test && npm run start",
    "be": "webpack && babel src/serverside/ --out-dir src/dist/ --presets=es2015 --copy-files && electron .",
    "package-mac": "webpack && babel src/serverside/ --out-dir src/dist/ --presets=es2015 --copy-files && electron-packager . --overwrite --platform=darwin --arch=x64 --icon=Heep_Gradient.icns --prune=true --out=release-builds",
    "package-win": "webpack && babel src/serverside/ --out-dir src/dist/ --presets=es2015 --copy-files && electron-packager . Heep --overwrite --asar --platform=win32 --arch=ia32 --icon=Heep_Gradient.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='Heep Desktop'",
    "package-win-dev": "webpack && babel src/serverside/ --out-dir src/dist/ --presets=es2015 --copy-files && electron-packager . HeepDev --overwrite --asar --platform=win32 --arch=ia32 --icon=Heep_Gradient.ico --prune=true --out=development-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='Heep Desktop'"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "electron-log": "^2.2.14",
    "express": "^4.16.2",
    "firebase": "^4.3.0",
    "firebaseui": "^2.3.0",
    "immutable": "^3.8.1",
    "jquery": "^3.1.1",
    "react": "^15.3.2",
    "react-bootstrap": "^0.31.3",
    "react-dom": "^15.3.2",
    "react-redux": "^5.0.2",
    "react-router": "^4.2.0",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.22.0",
    "css-loader": "^0.25.0",
    "dateformat": "^3.0.2",
    "electron": "^1.7.12",
    "electron-packager": "^11.0.0",
    "file-loader": "^0.11.2",
    "fs": "0.0.1-security",
    "history": "^4.7.2",
    "mocha": "^3.2.0",
    "mocha-webpack": "^0.7.0",
    "node-sass": "^4.5.0",
    "normalizr": "^3.1.0",
    "plotly.js": "^1.33.0",
    "raw-loader": "^0.5.1",
    "react-addons-update": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-file-download": "^0.3.4",
    "react-plotly.js": "^1.3.0",
    "redux-devtools-extension": "^2.13.2",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "underscore": "^1.8.3",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.13.0",
    "webpack-node-externals": "^1.5.4"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "React",
    "Redux",
    "Webpack",
    "Babel",
    "Immutable",
    "node"
  ],
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ]
  },
  "main": "electron_main.js"
}
