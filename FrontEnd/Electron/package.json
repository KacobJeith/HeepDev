  "scripts": {
    "test": "mocha-webpack --webpack-config webpack.config-test.js \"src/**/*.test.js\"",
    "build": "npm run copy-files && webpack --config webpack.config.js && webpack --config webpack-electron.config.js",
    "dev": "node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --watch-poll --watch",
    "start": "electron .",
    "tbs": "npm run test && npm run bs",
    "bs": "webpack && npm run start",
    "ts": "npm run test && npm run start",
    "be": "npm run build && electron ./dist/",
    "copy-files": "babel ./src/assets/ --out-dir ./dist/assets/ --copy-files",
    "dist": "electron-builder --projectDir ./dist/ -p always",
    "package-all": "npm run build && electron-builder --projectDir ./dist/ -mw -p always",
    "package-current": "npm run build && npm run dist",
    "build-electron-bundle": "webpack --config webpack-electron.config.js"
  },
